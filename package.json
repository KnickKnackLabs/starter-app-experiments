{
  "name": "starter",
  "private": true,
  "devDependencies": {
    "@biomejs/biome": "2.3.8",
    "@types/react": "^19.1.0",
    "typescript": "^5.2.2",
    "ultracite": "6.4.0"
  },
  "scripts": {
    "lint": "biome check .",
    "lint:fix": "biome check --write .",
    "typecheck": "pnpm -r exec tsc --noEmit",
    "check": "pnpm lint && pnpm typecheck",
    "native": "pnpm --filter expo-app run start",
    "native:ios": "pnpm --filter expo-app run ios",
    "native:android": "pnpm --filter expo-app run android",
    "native:storybook": "pnpm --filter expo-app run storybook",
    "web": "pnpm --filter web-app run dev",
    "web:storybook": "pnpm --filter web-app run storybook",
    "desktop": "pnpm --filter desktop-app run start",
    "desktop:package": "pnpm --filter desktop-app run package",
    "desktop:make": "pnpm --filter desktop-app run make",
    "desktop:storybook": "pnpm --filter desktop-app run storybook"
  },
  "pnpm": {
    "overrides": {
      "react": "19.1.0",
      "react-dom": "19.1.0",
      "@types/react": "19.1.17",
      "@types/react-dom": "19.1.3"
    },
    "onlyBuiltDependencies": [
      "electron",
      "electron-winstaller",
      "esbuild"
    ]
  },
  "packageManager": "pnpm@10.26.1"
}
