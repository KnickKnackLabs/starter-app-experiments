{
  "name": "starter",
  "private": true,
  "devDependencies": {
    "@biomejs/biome": "2.3.8",
    "@types/react": "^19.1.0",
    "typescript": "^5.2.2",
    "ultracite": "6.4.0"
  },
  "scripts": {
    "lint": "biome check .",
    "lint:fix": "biome check --write .",
    "typecheck": "pnpm -r exec tsc --noEmit",
    "check": "pnpm lint && pnpm typecheck",
    "dev": "pnpm --filter web-app run dev",
    "build": "pnpm --filter web-app run build",
    "start": "pnpm --filter web-app run start",
    "storybook": "pnpm --filter web-app run storybook"
  },
  "pnpm": {
    "overrides": {
      "react": "19.1.0",
      "react-dom": "19.1.0",
      "@types/react": "19.1.17",
      "@types/react-dom": "19.1.3"
    },
    "onlyBuiltDependencies": [
      "esbuild"
    ]
  },
  "packageManager": "pnpm@10.26.1"
}
